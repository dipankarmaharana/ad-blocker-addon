{
    "manifest_version": 2,
    "name": "Noteworthy",
    "version": "0.1",
    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["content.js"]
        }
      ],
      "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "pencil.png"
      },
    "permissions": [
      "storage",
      "tabs"
    ] 
}  